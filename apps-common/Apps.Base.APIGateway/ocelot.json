{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "apps.basic.service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/Basic/{url}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete", "Options" ]
    },
    {
      "DownstreamPathTemplate": "/File",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "apps.filesystem.service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/Files",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete", "Options" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://api.mybusiness.com"
  }
}
